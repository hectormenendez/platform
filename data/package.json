{
    "name": "data",
    "version": "0.0.1",
    "main": "src/index.js",
    "scripts": {
        "start": "postgraphile",
        "dbvs:seed": "knex seed:run --specific=_root.js",
        "dbvs:first": "knex migrate:rollback --all",
        "dbvs:last": "knex migrate:latest",
        "dbvs:next": "knex migrate:up --",
        "dbvs:prev": "knex migrate:down --",
        "dbvs:reset": "npm run -s dbvs:first && npm run -s dbvs:last && npm run -s dbvs:seed"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "knex": "^0.21.2",
        "pg": "^8.3.0"
    },
    "devDependencies": {
        "@graphile-contrib/pg-simplify-inflector": "^6.0.0",
        "@mermaid-js/mermaid-cli": "^8.5.3",
        "postgraphile": "^4.7.0"
    }
}
