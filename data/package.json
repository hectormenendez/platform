{
    "name": "data",
    "version": "0.0.1",
    "main": "src/index.js",
    "scripts": {
        "parent:start": "node $npm_package_main",
        "build:schema": "mmdc -i ./schema.mmd -o ./.files/schema.svg",
        "dbvs:add": "knex migrate:make $(git branch --show-current)",
        "dbvs:seed": "knex seed:run --specific=_root.js",
        "dbvs:list": "knex migrate:list",
        "dbvs:first": "knex migrate:rollback --all",
        "dbvs:last": "knex migrate:latest",
        "dbvs:next": "knex migrate:up --",
        "dbvs:prev": "knex migrate:down --",
        "dbvs:reset": "npm run -s dbvs:first && npm run -s dbvs:last && npm run -s dbvs:seed"
    },
    "dependencies": {
        "dotenv": "^8.2.0",
        "knex": "^0.21.2",
        "pg": "^8.3.0"
    },
    "devDependencies": {
        "@mermaid-js/mermaid-cli": "^8.5.3"
    }
}
